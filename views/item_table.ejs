<table class="table table-striped table-bordered table-condensed">
    <thead>
        <tr>
          <% columns.forEach( column => { %>
            <th> <%= column %> </th>
          <% }); %>
        </tr>
    </thead>
    <% items.forEach( item => { %>
      <tbody>
          <tr>
              <td><%= item.item_id %></td>
              <td><%= item.item_name %></td>
              <td><img src="<%= item.item_picture %>" alt="" width="100px" height="100px"></td>
              <td><%= item.item_price %></td>
              <td><%= item.item_stock %></td>
              <td>
                  <a href="edit/<%= item.item_id %>" class="btn btn-warning"><i class="icon-edit icon-white"></i> Edit</a>
                  <a href="#" class="btn btn-danger" onclick="deleteById(<%=item.item_id%>)"><i class="icon-trash icon-white"></i> Delete</a>
              </td>
          </tr>
      </tbody>
    <% }); %>

</table>
